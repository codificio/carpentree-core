        }
    ],
    "homepage": "https://github.com/codificio/carpentree-core",
    "keywords": ["Laravel", "Carpentree", "Core"],
    "require": {
        "php": "^7.1.3",
        "algolia/algoliasearch-client-php": "^2.2",
        "barryvdh/laravel-cors": "^0.11.2",
        "dimsav/laravel-translatable": "^9.3",
        "kalnoy/nestedset": "^4.3",
        "illuminate/support": "~5",
        "laravel/socialite": "^4.0",
        "laravel/passport": "^7.1",
        "laravel/scout": "^7.1",
        "hivokas/laravel-passport-social-grant": "^1.1",
        "illuminate/contracts": "^5.7",
        "illuminate/http": "^5.7",
        "illuminate/notifications": "^5.7",
        "illuminate/auth": "^5.7",
        "illuminate/container": "^5.7",
        "illuminate/database": "^5.7",
        "doctrine/dbal": "^2.9",
        "laravel/tinker": "^1.0",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-model-status": "^1.6",
        "spatie/laravel-permission": "^2.30",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Carpentree\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carpentree\\Core\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Carpentree\\Core\\Providers\\CoreServiceProvider",
                "Carpentree\\Core\\Providers\\EventServiceProvider",
                "Carpentree\\Core\\Providers\\AuthServiceProvider",
                "Carpentree\\Core\\Providers\\LocaleServiceProvider"
            ]
        }
    }
}
