{
    "name": "codificio/carpentree-core",
    "description": "Core package of Caprentree Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Enrico Nardo",
            "email": "enrico@codificio.com",
            "homepage": "https://codificio.com"
        }
    ],
    "homepage": "https://github.com/codificio/carpentree-core",
    "keywords": ["Laravel", "Carpentree", "Core"],
    "require": {
        "algolia/algoliasearch-client-php": "^2.2",
        "barryvdh/laravel-cors": "^0.11.2",
        "astrotomic/laravel-translatable": "^11.9",
        "kalnoy/nestedset": "^5.0",
        "illuminate/support": "^6.0",
        "laravel/socialite": "^5.5",
        "laravel/passport": "^9.3.2",
        "laravel/scout": "^8.6",
        "coderello/laravel-passport-social-grant": "^3.0",
        "illuminate/contracts": "^6.0",
        "illuminate/http": "^6.0",
        "illuminate/notifications": "^6.0",
        "illuminate/auth": "^6.0",
        "illuminate/container": "^6.0",
        "illuminate/database": "^6.0",
        "doctrine/dbal": "^2.10",
        "laravel/tinker": "^1.0",
        "plank/laravel-mediable": "^4.3",
        "spatie/laravel-permission": "^3.18",
        "predis/predis": "^1.1",
        "teamtnt/laravel-scout-tntsearch-driver": "^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Carpentree\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carpentree\\Core\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Carpentree\\Core\\Providers\\CoreServiceProvider",
                "Carpentree\\Core\\Providers\\EventServiceProvider",
                "Carpentree\\Core\\Providers\\AuthServiceProvider",
                "Carpentree\\Core\\Providers\\LocaleServiceProvider"
            ]
        }
    }
}
